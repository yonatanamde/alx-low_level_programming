This is a project regarding pointers, string and array

#include "main.h"
/**
 * reset_to_98 - takes a pointer and update the value
 * @n: pointer
 * Return: void
 */
void reset_to_98(int *n)
{
	*n = 98;
}

#include "main.h"
/**
 * swap_int - changes the value of two integers
 * @a: integer
 * @b: integer
 * Return: void
 */
void swap_int(int *a, int *b)
{
	int c = *a;
	*a = *b;
	*b = c;
}

#include "main.h"
/**
 * _strlen - prints the length of string
 * @s: character
 * Return: integer
 */
int _strlen(char *s)
{
	int i;

	for (i = 0; s[i] != '\0'; i++)
	{
	}

	return (i);
}


#include "main.h"
/**
 * _puts - prints strings
 * @str: character
 * Return: void
 */
void _puts(char *str)
{
	int i;

	for (i = 0; str[i] != '\0'; i++)
	{
		putchar((str[i]));
	}
	putchar('\n');
}

#include "main.h"
/**
 * print_rev - prints the reverse of string
 * @s: character pointer
 * Return: void
 */
void print_rev(char *s)
{
	int i;
	int a;

	for (i = 0; s[i] != '\0'; i++)
	{
	}

	for (a = i - 1; a >= 0; a--)
	{
		putchar(s[a]);
	}
	putchar('\n');
}

#include "main.h"
/**
 * rev_string - reverse a string
 * @s: character pointer
 * Return: void
 */
void rev_string(char *s)
{
	int i;
	int a;
	char *start, *end, ch;

	start = s;
	end = s;

	for (i = 0; s[i] != '\0'; i++)
	{
	}
	for (a = 0; a < i - 1; a++)
		end++;

	for (a = 0; a < i / 2; a++)
	{
		ch = *end;
		*end = *start;
		*start = ch;
		start++;
		end--;
	}
}

#include "main.h"
/**
 * puts2 - prints even string
 * @str: char pointer
 * Return: void
 */
void puts2(char *str)
{
	int i;

	for (i = 0; str[i] != '\0'; i += 2)
	{
		if (i > 5 && str[i] == 'H')
			break;
		putchar(str[i]);
	}
	putchar('\n');
}
