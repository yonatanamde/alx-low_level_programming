#include "main.h"
/**
 * puts_half - prints half of string
 * @str: char pointer
 * Return: void
 */
void puts_half(char *str)
{
	int i;
	int n;
	int m;

	for (i = 0; str[i] != '\0'; i++)
	{
	}

	n = i;

	if (n % 2 == 0)
		n = i / 2;
	else
		n = ((i - 1) / 2) + 1;

	for (m = n; str[m] != '\0'; m++)
	{
		putchar(str[m]);
	}
	putchar('\n');
}

#include "main.h"
/**
 * print_array - prints the element of array
 * @a: integer pointer
 * @n: integer
 */
void print_array(int *a, int n)
{
	int i;

	for (i = 0; i < n; i++)
	{
		printf("%d", a[i]);
		if (i != n - 1)
			printf(", ");
	}
	putchar('\n');
}

#include "main.h"
/**
 * _strcpy - copies string
 * @dest: char pointer
 * @src: char pointer
 * Return: char pointer
 */
char *_strcpy(char *dest, char *src)
{
	int i;
	int n;

	for (n = 0; src[n] != '\0'; n++)
	{
	}

	for (i = 0; i < n && src[i] != '\0'; i++)
		dest[i] = src[i];

	for ( ; i <= n; i++)
		dest[i] = '\0';

	return (dest);
}

#include "main.h"
/**
 * _atoi - converts string to integer
 * @s: character pointer
 * Return: integer
 */
int _atoi(char *s)
{
	int i, sum = 0;
	int exist = 0;
	int c = 1;

	for (i = 0; s[i] != '\0'; i++)
	{
		if (s[i] == 43 || s[i] == 45)
		{
			c = s[i];
		}

		if (s[i] >= '0' && s[i] <= '9')
		{
			sum = sum * 10 + (s[i] - 48);
			exist = 1;
		}
	}
	sum *= c;

	if (exist == 0)
		return (0);
	else
		return (sum);
}

#include "main.h"
